#!/usr/bin/env bash

if
  [[ -s "/Users/gcode/EH/simpleapp/vendor/bundle/ruby/2.6.0/environment" ]]
then
  source "/Users/gcode/EH/simpleapp/vendor/bundle/ruby/2.6.0/environment"
  exec gem "$@"
else
  echo "ERROR: Missing RVM environment file: '/Users/gcode/EH/simpleapp/vendor/bundle/ruby/2.6.0/environment'" >&2
  exit 1
fi
